{
    /* editor */
    "editor.fontSize": 14,
    "editor.lineHeight": 24,
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 3,
    "editor.cursorBlinking": "smooth",
    "editor.renderWhitespace": "none",
    "editor.stickyScroll.enabled": true,
    "editor.minimap.enabled": false,
    "editor.foldingMaximumRegions": 7000,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'JetBrains Mono', 'LINE Seed Sans KR', 'Roboto Mono', 'hack'",
    "editor.wordWrap": "off",
    "editor.wordWrapColumn": 120,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.mouseWheelZoom": true,
    "editor.autoClosingDelete": "always",
    "editor.autoClosingOvertype": "always",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.tokenColorCustomizations": {
        "comments": "#FFC1CC",
        "textMateRules": [
            {
                "scope": ["comment"],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.codeActionsOnSave": {
        "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
        "diffEditor.ignoreTrimWhitespace": false,
        "explorer.sortOrder": true,
        "source.fixAll.eslint": true,
        "source.fixAll.stylelint": true
    },

    /* workbench */
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.wrapTabs": true,
    "workbench.startupEditor": "none",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.editor.labelFormat": "short",
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.iconTheme": "webstorm-icons",
    "workbench.colorCustomizations": {
        "activityBar.background": "#1E1E1E",
        "activityBarBadge.foreground": "#FFFFFF",
        "button.background": "#0E639C",
        "editor.background": "#1E1E1E",
        "editor.lineHighlightBackground": "#2A2D2E",
        "editorGroupHeader.tabsBackground": "#1E1E1E",
        "editorCursor.foreground": "#FFFFFF",
        "editorBracketMatch.border": "#FFFFFF",
        "editorWhitespace.foreground": "#404040",
        "list.hoverBackground": "#2C2C2C",
        "list.focusBackground": "#2C2C2C",
        "panel.background": "#1E1E1E",
        "panel.border": "#808080",
        "sideBar.background": "#1E1E1E",
        "sideBar.foreground": "#eee",
        "sideBarSectionHeader.background": "#2C2C2C",
        "statusBar.background": "#2C2C2C",
        "statusBar.noFolderBackground": "#2C2C2C",
        "tab.activeBackground": "#1E1E1E",
        "tab.inactiveBackground": "#2C2C2C",
        "terminal.background": "#1E1E1E",
        "titleBar.activeBackground": "#1E1E1E",
        "titleBar.activeForeground": "#D4D4D4",
        "titleBar.inactiveBackground": "#1E1E1E",
        "titleBar.inactiveForeground": "#D4D4D4",
        "diffEditor.insertedTextBackground": "#00800010",
        "diffEditor.removedTextBackground": "#FF000010",
        "terminal.ansiBlack": "#eaeaeb",
        "terminal.ansiBlue": "#81A2BE",
        "terminal.ansiCyan": "#8ABEB7",
        "terminal.ansiGreen": "#7de8b2",
        "terminal.ansiMagenta": "#B294BB",
        "terminal.ansiRed": "#DB2D20",
        "terminal.ansiWhite": "#C5C8C6",
        "terminal.ansiYellow": "#F0C674",
        "terminal.ansiBrightBlack": "#eaeaeb",
        "terminal.ansiBrightBlue": "#81A2BE",
        "terminal.ansiBrightCyan": "#8ABEB7",
        "terminal.ansiBrightGreen": "#7de8b2",
        "terminal.ansiBrightMagenta": "#B294BB",
        "terminal.ansiBrightRed": "#DB2D20",
        "terminal.ansiBrightWhite": "#C5C8C6",
        "terminal.ansiBrightYellow": "#F0C674",
        "gitDecoration.deletedResourceForeground": "#b30000",
        "gitDecoration.addedResourceForeground": "#008fb3",
        "gitDecoration.modifiedResourceForeground": "#ffa600",
        "gitDecoration.untrackedResourceForeground": "#36d436"
    },

    /* files */
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 3000,
    "files.trimTrailingWhitespace": true,
    "files.eol": "\n",

    /* terminal */
    "terminal.explorerKind": "external",
    "terminal.integrated.fontFamily": "\"JetBrainsMono NF\", \"JetBrainsMono Nerd Font\"",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.external.windowsExec": "C:\\Users\\forbiz\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
    "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Windows PowerShell": {
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },

    /* explorer */
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,

    /* emmet */
    "emmet.preferences": {},
    "emmet.syntaxProfiles": {
        "javascript": "js"
        // "javascript": "jsx"
    },
    "emmet.includeLanguages": {
        "javascript": "javascript"
        // "javascript": "javascriptreact"
    },

    /* ETC */
    "security.workspace.trust.untrustedFiles": "open",
    "html.format.enable": false,
    "html.format.wrapAttributes": "force-aligned",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",

    /* EXTENSION : prettier */
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.format.enable": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "prettier.printWidth": 120,
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,
    "prettier.semi": true,
    "prettier.arrowParens": "always",
    "prettier.singleQuote": true,
    "prettier.jsxSingleQuote": true,
    "prettier.trailingComma": "all",
    "prettier.bracketSpacing": true,
    "prettier.proseWrap": "always",
    "prettier.endOfLine": "auto",
    "prettier.vueIndentScriptAndStyle": true,
    "prettier.bracketSameLine": true,

