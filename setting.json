    /* EXTENSION : Todo highlight Color */
    "todohighlight.keywords": [
        {
            "text": "NOTI:",
            "color": "#000",
            "fontWeight": "bold",
            "backgroundColor": "#79d49e",
            "overviewRulerColor": "grey"
        },
        {
            "text": "WARN:",
            "color": "#000",
            "fontWeight": "bold",
            "backgroundColor": "#f7e389",
            "isWholeLine": false
        },
        {
            "text": "MEMO:",
            "color": "#000",
            "fontWeight": "bold",
            "backgroundColor": "#f38134",
            "isWholeLine": false
        },
        {
            "text": "TODO:",
            "color": "#ff7b54",
            "border": "1px solid rgba(255, 255, 255, 0.8)",
            "fontWeight": "bold",
            "borderRadius": "2px",
            "backgroundColor": "rgba(0,0,0,.2)"
        },
        {
            "text": "FIXME:",
            "color": "#f33434",
            "fontWeight": "bold",
            "border": "1px solid #f33434",
            "borderRadius": "2px",
            "backgroundColor": "rgba(0,0,0,.2)"
        }
    ],
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss"
    ],
    "todohighlight.exclude": [
        "**/node_modules/**",
        "**/bower_components/**",
        "**/dist/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map",
        "**/.next/**"
    ],

    /* EXTESNSION : TODO Tree */
    "todo-tree.highlights.enabled": false,

    /* EXTENSION : AiCodeHelper */
    "aicodehelper.language": "korean",
    "aicodehelper.gptkey": "****",

    /* EXTENSION : Auto Close Tag */
